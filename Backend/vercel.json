{
    "version": 2,
    "builds": [
      {
        "src": "Backend/Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "Frontend/dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Backend/Server.js"
      }
    ]
  }
  